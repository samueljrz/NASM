%include "io.inc"

section .data
    NumFat DD 0d
    Aux DD 1d

section .text
global CMAIN
CMAIN:
    mov ebp, esp; for correct debugging
    GET_DEC 4, [NumFat]
    
    mov eax, 1
    mov ebx, [NumFat]
    
    mov edx, 0
    mov ecx, ebx
    mul ecx 
    sub ebx, 1
      

    PRINT_DEC 4, eax
    
    xor eax, eax
    ret
